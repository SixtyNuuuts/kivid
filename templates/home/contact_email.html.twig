{% extends 'email/emailBase.html.twig' %}

{% block content %}
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
            <td class="h1 pb25" style="color:#dcd4c1; font-family:'Muli', Arial,sans-serif; font-size:30px; line-height:32px; text-align:center; padding-bottom:25px;">
                {{ patient }} a besoin d'un praticien
            </td>
        </tr>
        <tr>
            <td class="text-center pb25" style="color:#0c2956; font-family:'Muli', Arial,sans-serif; font-size:16px; line-height:26px; text-align:center; padding-bottom:25px;">
               {{ contactMessage }}
                {% if contactTel %}
                    <br>Tel : {{ contactTel }}
                {% endif %}
            </td>
        </tr>
    </table>
{% endblock %}
