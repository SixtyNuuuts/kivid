{% extends 'base.html.twig' %}

{% block layout %}
    {% include 'layout/partial/_navbar.html.twig' %}
    {% block body %}{% endblock %}
	{% if app.request.get('_route') != 'app_home' %}
		{% include 'layout/partial/_footer.html.twig' %}
	{% endif %}
{% endblock %}

{% block stylesheets %}
	{{ parent() }}

	{{ encore_entry_link_tags('navbar') }}
{% endblock %}

{% block javascripts %}
	{{ parent() }}

	{{ encore_entry_script_tags('navbar') }}
{% endblock %}
